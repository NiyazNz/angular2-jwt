{
  "name": "@auth0/angular-jwt",
  "version": "3.0.0",
  "description": "JSON Web Token helper library for Angular",
  "scripts": {
    "ng": "ng",
    "build": "ng build",
    "build:prod": "npm run build && npm run copy-files",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "watch": "npm run build -- --watch",
    "copy-lic": "cp LICENSE dist/angular-jwt/LICENSE",
    "copy-readme": "cp README.md dist/angular-jwt/README.md",
    "copy-files": "npm run copy-lic && npm run copy-readme",
    "publish": "cd dist/angular-jwt && npm publish --access=public"
  },
  "private": true,
  "repository": {
    "type": "git",
    "url": "git+https://github.com/auth0/angular2-jwt"
  },
  "keywords": [
    "angular",
    "angular 2",
    "authentication",
    "jwt"
  ],
  "author": "Ryan Chenkie",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/auth0/angular2-jwt/issues"
  },
  "homepage": "https://github.com/auth0/angular2-jwt",
  "dependencies": {
    "@angular/common": "~8.2.4",
    "@angular/compiler": "~8.2.4",
    "@angular/core": "~8.2.4",
    "@angular/platform-browser": "~8.2.4",
    "@angular/platform-browser-dynamic": "~8.2.4",
    "rxjs": "~6.4.0",
    "tslib": "^1.10.0",
    "zone.js": "~0.9.1",
    "url": "~0.11.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.803.3",
    "@angular-devkit/build-ng-packagr": "~0.803.3",
    "@angular/cli": "~8.3.3",
    "@angular/compiler-cli": "~8.2.4",
    "@angular/language-service": "~8.2.4",
    "@types/node": "~8.9.4",
    "@types/jasmine": "~3.3.8",
    "@types/jasminewd2": "~2.0.3",
    "codelyzer": "^5.0.0",
    "jasmine-core": "~3.4.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~4.1.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.1",
    "karma-jasmine": "~2.0.1",
    "karma-jasmine-html-reporter": "^1.4.0",
    "ng-packagr": "^5.4.0",
    "protractor": "~5.4.0",
    "ts-node": "~7.0.0",
    "tsickle": "^0.37.0",
    "tslint": "~5.15.0",
    "typescript": "~3.5.3"
  }
}
